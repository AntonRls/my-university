{
  "name": "my-university",
  "sourceRoot": "apps/my-university/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/my-university",
        "configFile": "apps/my-university/vite.config.ts"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "configFile": "apps/my-university/vite.config.ts"
      },
      "configurations": {
        "development": {
          "buildTarget": "my-university:build:development"
        },
        "production": {
          "buildTarget": "my-university:build:production"
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "defaultConfiguration": "production",
      "options": {
        "configFile": "apps/my-university/vite.config.ts"
      },
      "configurations": {
        "development": {
          "buildTarget": "my-university:build:development"
        },
        "production": {
          "buildTarget": "my-university:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/my-university/src/**/*.{ts,tsx,js,jsx}",
          "apps/my-university/vite.config.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/apps/my-university"],
      "options": {
        "configFile": "apps/my-university/vite.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "@nx/run-commands",
      "options": {
        "command": "tsc --noEmit -p apps/my-university/tsconfig.json"
      }
    }
  },
  "tags": ["scope:frontend", "type:app"]
}
