{
  "name": "api",
  "sourceRoot": "libs/api/src",
  "projectType": "library",
  "targets": {
    "typecheck": {
      "executor": "@nx/js:tsc",
      "outputs": ["{workspaceRoot}/dist/libs/api"],
      "options": {
        "project": "libs/api/tsconfig.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/api/src/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:api"]
}
